{
  "name": "monorepo",
  "version": "1.0.0",
  "description": "A react nonorepo based on webpack and typescript.",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "build": "lerna run build --stream",
    "tscheck": "lerna run tscheck --stream",
    "lint": "lerna run lint --stream --parallel",
    "release": "lerna version --force-publish",
    "publish": "lerna publish from-package --no-verify-access",
    "test": "jest",
    "example": "cd example && npm run start"
  },
  "files": [
    "dist/",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sep9/monorepo.git"
  },
  "author": "mike",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sep9/monorepo/issues"
  },
  "homepage": "https://github.com/sep9/monorepo#readme",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@types/fork-ts-checker-webpack-plugin": "^0.4.5",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.40",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "fork-ts-checker-webpack-plugin": "^7.2.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lerna": "^4.0.0",
    "prettier": "2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "peerDependencies": {
    "react": "v17.0.2"
  },
  "workspaces": ["packages/app", "packages/components", "packages/utils"]
